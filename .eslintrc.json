  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "ignorePatterns": ["*.config.js", "**/dist"],
  "rules": {
    "no-console":"error"
  }
}
